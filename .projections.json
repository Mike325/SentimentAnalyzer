{
  ".gitignore"                      : {"type": "Gitignore"},
  ".git/hooks/*"                    : {"type": "GitHooks"},
  ".git/config"                     : {"type": "Git"},
  ".git/info/*"                     : {"type": "Git"},
  ".projections.json"               : {"type": "Projections"},
  ".ycm_extra_conf.py"              : {"type": "ConfigYCM"},
  ".travis.yml"                     : {"type": "Travis"},
  ".github/workflows/main.yml"      : {"type": "Github"},
  "project.vim"                     : {"type": "Project"},
  "README.md"                       : {"type": "Readme"},
  "LICENSE"                         : {"type": "License"},
  "requirements/*.txt"              : {"type": "Requirements"},
  "Dockerfile"                      : {"type": "Docker"},
  "logger/*.py"                     : {"type": "Logger"},
  "service/systemd/*.timer"         : {"type": "Systemd"},
  "service/systemd/*.service"       : {"type": "Systemd"},
  "test/*"                          : {"type": "CI"},
  "test/test.sh"                    : {"type": "CI"},
  "manage.py"                       : {"type": "Manage"},
  "server/nlp/*.py"                 : {"type": "NLP"},
  "server/*.py"                     : {"type": "Server"},
  "server/settings.py"              : {"type": "Server"},
  "server/apps/*/models.py"         : {"type": "Models"},
  "server/apps/*/views.py"          : {"type": "Views"},
  "server/apps/*/apps.py"           : {"type": "Apps"},
  "server/apps/*/tests.py"          : {"type": "Tests"},
  "server/apps/crawler/apps.py"     : {"type": "Crawler"},
  "server/apps/crawler/*.py"        : {"type": "Crawler"},
  "server/apps/crawler/social/*.py" : {"type": "Network"},
  "server/apps/post/apps.py"        : {"type": "Post"},
  "server/apps/post/*.py"           : {"type": "Post"}
}
